  },
  "devDependencies": {
    "@babel/types": "^7.27.0",
    "@types/babel__core": "^7.20.5",
    "vite": "^6.2.4"
  },
  "peerDependencies": {
    "vite": "^5.0.0 || ^6.0.0",
